				<section id="contenido">

					<a id="acontenido"></a>

					<h2 class="titu_formularios">Saldo en tarjeta</h2>

					
										<div class="contenido_capa">

						<h4 class="titu_contenido">
							<!--<span class="txt_numero_contenido">1</span>-->
							<span class="txt_titu_contenido">ACUMULADO Y BONOS</span>
						</h4>

						<div class="row">

							<p class="txt_grande">Acumulado:  €</p>

							<p class="txt_grande">Bonos disponibles: 0</p>

							<p>Bonos consumidos: 0</p>

							<p>Bonos conseguidos: 0</p>

						</div>

					</div>

					<div class="contenido_capa cont_fondo_gris">

						<h4 class="titu_contenido">
							<!--<span class="txt_numero_contenido">2</span>-->
							<span class="txt_titu_contenido">BÚSQUEDA POR FECHAS</span>
